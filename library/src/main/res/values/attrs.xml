<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HKSpinner">
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
        <attr name="android:layout_height"/>
        <attr name="arrowDrawable" format="reference"/>
        <attr name="arrowPadding" format="dimension"/>
        <attr name="arrowMarginVertical" format="dimension"/>
        <attr name="android:divider"/>
        <attr name="android:dividerHeight"/>
        <attr name="android:gravity"/>
        <attr name="android:padding"/>
        <attr name="android:paddingLeft"/>
        <attr name="android:paddingTop"/>
        <attr name="android:paddingBottom"/>
        <attr name="android:paddingRight"/>
    </declare-styleable>
    <declare-styleable name="RoundCornerTabLayout">
        <attr name="cornerRadius" format="dimension"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="dividerColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
    <declare-styleable name="SpinView">
        <attr name="speed" format="float"/>
    </declare-styleable>
    <declare-styleable name="ExtendSearchView">
        <attr name="isShowSearchIcon" format="boolean"/>
        <attr name="android:textColor"/>
        <attr name="android:textColorHint"/>
        <attr name="android:textSize"/>
    </declare-styleable>
</resources>